<h1> New Party </h1>

<%= form_with model: @party do |f| %>
    <%= f.label :name %>:
    <%= f.text_field :name %>

    <%= f.date_field :date %><br>

    <%= f.radio_button :private, true %>
    <%= f.label :private %><br>
    <%= f.radio_button :private, false %>
    <%= f.label :public %><br>

    <%= f.submit %>
<% end %>
